<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>UsuariosControl</servlet-name>
        <servlet-class>Controlador.UsuariosControl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlUsuarioCRUD</servlet-name>
        <servlet-class>Controlador.ControlUsuarioCRUD</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorChartJS</servlet-name>
        <servlet-class>Controlador.ControladorChartJS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlChart_JS</servlet-name>
        <servlet-class>Controlador.ControlChart_JS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlProyectos</servlet-name>
        <servlet-class>Controlador.ControlProyectos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsuariosControl</servlet-name>
        <url-pattern>/UsuariosControl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlUsuarioCRUD</servlet-name>
        <url-pattern>/ControlUsuarioCRUD</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorChartJS</servlet-name>
        <url-pattern>/ControladorChartJS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlChart_JS</servlet-name>
        <url-pattern>/ControlChart_JS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlProyectos</servlet-name>
        <url-pattern>/ControlProyectos</url-pattern>
    </servlet-mapping>
</web-app>
