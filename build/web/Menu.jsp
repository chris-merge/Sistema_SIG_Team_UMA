.slider {
  width: 200px;
  height: 300px;
  background-color: yellow;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  overflow-y: scroll;
  padding: 10px;
  border-radius: 10px;
}

.slider ul {
  list-style-type: none;
  padding: 0;
  margin: 0;
}

.slider li {
  padding: 10px;
  border-bottom: 1px solid #ccc;
}

.slider li:last-child {
  border-bottom: none;
}
